<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bulma@0.9.1/css/bulma.min.css">
    <title>Table</title>
</head>
<body>
    
    <table class="table">
        <thead>
            <% header.forEach(col => { %>
                <th><%= col %></th>
            <% }) %>
        </thead>
        <tbody>
            <% rows.forEach(row => { %>
                <tr>
                    <% row.forEach(element => { %>
                        <td><%= element  %></td>
                    <% }) %>
                </tr>
            <% }) %>
        </tbody>
    </table>

</body>
</html>